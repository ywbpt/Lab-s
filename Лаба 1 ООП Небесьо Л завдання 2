using System;
class Program
{
 static void Main()
 {
 int n = 100; // Кількість чисел, які ви хочете знайти
 int count = 0; // Лічильник знайдених чисел
 int i = 1; // Початкове число послідовності
 Console.WriteLine($"Перші {n} чисел, що діляться на 7 і дають залишок 1, 2 або 5:");
 while (count < n)
 {
 if (i % 7 == 1 || i % 7 == 2 || i % 7 == 5)
 {
 Console.WriteLine(i);
 count++;
 }
 i++;
 }
 }
} 
